#include<stdio.h>

int main()
{
    int arr[10],i,sum=0;
    float avg;

    printf("Enter the elements of an array:  \n");

    for (i = 0; i < 10 ; i++)
    {
        scanf("%d\n",&arr[i]);
        sum += arr[i];
    }
    avg = sum /2;
    printf("Average = %2.f ", avg);
    return 0;
    
}
