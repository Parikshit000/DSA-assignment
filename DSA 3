#include<stdio.h>

int main ()
{
    int arr[10],i;
    printf("Enter the 10 element of an array :\n");

    for ( i = 0; i < 10; i++)
    {
        scanf("%d\n",&arr[i]);
    }
    printf("Even no. of an array is : \n");

    for ( i = 0; i < 10; i++)
    {
        if (arr[i]%2==0)
        {
            printf("%d \t ",arr[i]);
        }
    }
    return 0;
}
