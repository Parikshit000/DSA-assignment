#include <stdio.h>

int main() {
    // Create a 2D array
    int array_2d[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    // Print the elements where the sum of indices is even
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            if((i+j)%2 == 0) {
                printf("%d ", array_2d[i][j]);
            } else {
                printf(" \n");
            }
        }
        printf("\n");
    }

    return 0;
}
