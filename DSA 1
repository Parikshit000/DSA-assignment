#include<stdio.h>

int main()
{
    int a[5],i;

    printf("Enter the value of 5 subject : \n ");
    for (i = 1; i <= 5; i++)
    {
        printf("Enter %d subject numbers  (From 100): ",i);
        scanf("%d\n",&a[i]);
    }

    printf("Please check the marks you entered : \n");
    for ( i = 1; i <= 5; i++)
    {
        printf("Marks of %d subject is %d from 100. \n",i,a[i]);
    }
    
    return 0;
}
